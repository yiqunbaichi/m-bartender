{
  "apps": [
    {
      "name": "m-bartender-ws",
      "cwd": "../",
      "script": "ws/server.js",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "../logs/ws-app.stderr.log",
      "out_file": "../logs/ws-app.stdout.log",
      "pid_file": "../pids/ws-geo-api.pid",
      "instances": 1,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "500M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "fork",
      "autorestart": false,
      "vizion": false
    }
  ]
}